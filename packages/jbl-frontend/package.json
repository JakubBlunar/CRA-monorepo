{
  "name": "@jbl/frontend",
  "version": "0.1.0",
  "private": true,
  "author": "Jakub BlunÃ¡r",
  "main": "src/index.tsx",
  "scripts": {
    "start": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling craco start",
    "build": "cross-env NODE_ENV=production craco build",
    "test": "cross-env craco test",
    "lint": "cross-env eslint ./src/**/*.{ts,tsx} --format=codeframe",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@jbl/core": "0.1.0",
    "@jbl/ui": "0.1.0",
    "@fvilers/disable-react-devtools": "1.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@craco/craco": "5.9.0",
    "@testing-library/dom": "7.28.1",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.3.0",
    "@types/jest": "26.0.16",
    "@types/node": "13.1.6",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "react-scripts": "4.0.1",
    "source-map-explorer": "2.5.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/setupTests.{ts,tsx}",
      "!src/index.{ts,tsx}",
      "!src/serviceWorker.{ts,tsx}",
      "!src/**/*.(spec|test|stories).{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
